#include <iostream>
using namespace std;

int main() {
    int n, tq;
    cin >> n;

    int at[100], bt[100], rt[100];
    for(int i=0;i<n;i++){
        cin >> at[i] >> bt[i];
        rt[i] = bt[i];
    }

    cin >> tq;

    int ct[100], tat[100], wt[100];
    int time = 0, completed = 0;

    while(completed < n){
        bool done_any = false;

        for(int i=0;i<n;i++){
            if(at[i] <= time && rt[i] > 0){
                done_any = true;

                int x = min(tq, rt[i]);
                rt[i] -= x;
                time += x;

                if(rt[i] == 0){
                    ct[i] = time;
                    tat[i] = ct[i] - at[i];
                    wt[i]  = tat[i] - bt[i];
                    completed++;
                }
            }
        }

        if(!done_any) time++;
    }

    cout << "P\tAT\tBT\tCT\tTAT\tWT\n";
    for(int i=0;i<n;i++){
        cout << "P" << i << "\t"
             << at[i] << "\t"
             << bt[i] << "\t"
             << ct[i] << "\t"
             << tat[i] << "\t"
             << wt[i] << "\n";
    }
}
